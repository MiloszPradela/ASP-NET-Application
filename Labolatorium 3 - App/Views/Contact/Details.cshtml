@{
}
<h1> Szczegóły</h1>
<form asp-controller="Contact" asp-action="Details" method="post">
    <input asp-for="Id" />
    <div class="mb-3">
        <label asp-for="Name">Imię</label>
        <label class="form-control" asp-for="Name" />
    </div>
    <div class="mb-3">
        <label asp-for="Name">Adres email</label>
        <input class="form-control" asp-for="Email" />
    </div>
    <div class="mb-3">
        <label asp-for="Name">Telefon</label>
        <input class="form-control" asp-for="Phone" />
    </div>
    <div class="mb-3">
        <label asp-for="Name">Data urodzenia</label>
        <input class="form-control" asp-for="Birth"/>
    </div>
    <div class="mb-3">
        <button class=" btn btn-primary" asp-action="Index">Powrót</button>
    </div>
</form>